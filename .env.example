# Algorand API Node for accessing the blockchain
ALGORAND_NODE_URL=https://mainnet-api.algonode.cloud
ALGORAND_INDEXER_URL=https://mainnet-idx.algonode.cloud

# AI API Keys - You need at least one API key to use the application
# For Claude API (Anthropic)
CLAUDE_API_KEY=your_claude_api_key_here

# For OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# For Google Gemini API 
GEMINI_API_KEY=your_gemini_api_key_here

# You can leave API keys you don't plan to use as empty

# Redis cache configuration
REDIS_URL=redis://localhost:6379
REDIS_CACHE_EXPIRATION=86400 # Cache expiration in seconds (24 hours)

